public class Solution {
	
	public static int gcd(int a,int b)
	{
		int n = 1;
        int hcf = 1;
        int smaller = (a > b)? b : a ;
        
        for (int i = 0 ; i <= smaller ; i++){
            if (a % n == 0 && b % n == 0){
                hcf = n ;

            }
            n++;

        }
        return hcf;
	}		
}
