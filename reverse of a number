import java.util.Scanner;

public class Main {
	
	public static void main(String[] args) {
		
		Scanner scn = new Scanner (System.in);
		int n = scn.nextInt();
		int reserved_no = 0;
		while ( n > 0) {

			int last = n % 10 ;
			n = n / 10 ;
			reserved_no = (reserved_no * 10) + last ;
			
		}
		System.out.println(reserved_no);	 

	}
}
